{
  "version" : 2
  "functions": {
  
    "api/php/*.php": {
      "runtime":"vercel-php@0.5.2",
      "maxDuration": 5
    }

    
    
  },

  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/about",
      "dest": "/api/about.js"
    },
    {
      "src": "/php",
      "dest": "/api/index.php"
    }
  ]

}