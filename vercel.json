{
  "version" : 2,
  
  "functions": {
  
    "api/js/*.js": {
      "memory": 128,
      "maxDuration": 10
    },

     "api/php/*.php": {
      "runtime":"vercel-php@0.5.2",
      "maxDuration": 5
    },
    
  },

  "routes": [
    {
      "src": "/",
      "dest": "/api"
    },
    {
      "src": "/about",
      "dest": "/api/about.js"
    }
    
  ]

}